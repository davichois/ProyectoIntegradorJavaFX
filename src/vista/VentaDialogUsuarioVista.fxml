<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.VentaDialogUsuarioController">
   <children>
      <TextField fx:id="txtBusqueda" layoutX="348.0" layoutY="44.0" onKeyReleased="#busquedaProducto" prefHeight="25.0" prefWidth="233.0" promptText="Buscar" />
      <TableView fx:id="tblUsuario" layoutX="18.0" layoutY="76.0" prefHeight="279.0" prefWidth="565.0">
         <columns>
            <TableColumn fx:id="colID" prefWidth="28.0" text="ID" />
            <TableColumn fx:id="colNombre" prefWidth="119.0" text="Nombre" />
            <TableColumn fx:id="colApellido" prefWidth="133.0" text="Apellidos" />
            <TableColumn fx:id="colDni" prefWidth="90.0" text="DNI" />
            <TableColumn fx:id="colTelefono" prefWidth="92.0" text="Telefono" />
            <TableColumn fx:id="colIDZona" prefWidth="38.0" text="Zona" />
            <TableColumn fx:id="colSexo" prefWidth="73.0" text="Sexo" />
         </columns>
      </TableView>
      <Button fx:id="btnAgregar" layoutX="522.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#agregar" text="Agregar" />
      <Label layoutX="18.0" layoutY="18.0" prefHeight="26.0" prefWidth="220.0" text="Buscador de Usuario Never">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Button fx:id="btnSalir" layoutX="18.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#salir" text="Salir" />
   </children>
</AnchorPane>
