<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controlador.VentaDialogProductoController">
   <children>
      <TextField fx:id="txtBusqueda" layoutX="348.0" layoutY="44.0" onKeyReleased="#busquedaProducto" prefHeight="25.0" prefWidth="233.0" promptText="Buscar" />
      <TableView fx:id="tblProducto" layoutX="18.0" layoutY="76.0" prefHeight="279.0" prefWidth="565.0">
        <columns>
          <TableColumn fx:id="colID" prefWidth="28.0" text="ID" />
            <TableColumn fx:id="colNombre" prefWidth="121.0" text="Nombre" />
            <TableColumn fx:id="colPrecio" prefWidth="57.0" text="Precio" />
            <TableColumn fx:id="colStock" prefWidth="60.0" text="Stock" />
            <TableColumn fx:id="colDescription" prefWidth="115.0" text="Description" />
            <TableColumn fx:id="colCodigoBarra" prefWidth="113.0" text="Cod.Barra" />
            <TableColumn fx:id="colIDCategoria" prefWidth="70.0" text="Categoria" />
        </columns>
      </TableView>
      <Button fx:id="btnAgregar" layoutX="522.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#agregar" text="Agregar" />
      <Label layoutX="18.0" layoutY="18.0" text="Buscador de Productos Never">
         <font>
            <Font name="System Bold" size="17.0" />
         </font>
      </Label>
      <Button fx:id="btnSalir" layoutX="18.0" layoutY="361.0" mnemonicParsing="false" onMouseClicked="#salir" text="Salir" />
   </children>
</AnchorPane>
